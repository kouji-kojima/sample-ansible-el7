# here we assign variables to particular groups

#-----------
# 変数設定(vars)
#-----------

#Proxy URL
proxy_host: proxy_host
proxy_port: port_no

http_proxy: http://{{ proxy_host }}:{{ proxy_port }}/

#No Proxy
no_proxys: xxxxx.co.jp,yyyy.co.jp

#社内CA証明書 URL(Internal Certification Authority URL)
ca_url: https://xxxxxxxx.co.jp/xxx.ca
ca_sha256: xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx